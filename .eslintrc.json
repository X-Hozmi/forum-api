{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 4],
        "no-underscore-dangle": ["error", {
            "allow": ["_exports"],
            "allowAfterThis": true,
            "allowAfterSuper": true
        }],
        "camelcase": "off",
        "class-methods-use-this": "off",
        "no-unused-vars": ["warn", { "args": "none" }],
        "max-len": ["warn", { "code": 200 }]
    }
}
